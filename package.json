{
  "name": "ros_gazebo_gym_ws",
  "version": "2.2.1",
  "description": "Workspace package for the ros_gazebo_gym and ros_gazebo_gym_examples packages.",
  "keywords": [
    "reinforcement learning",
    "RL",
    "robotics",
    "simulation",
    "gymnasium",
    "gymnasium-environments",
    "openai-gym",
    "gym-environments",
    "gym",
    "ros",
    "gazebo",
    "environments",
    "ros-gazebo-gym",
    "panda",
    "emika-franka",
    "catkin workspace"
  ],
  "homepage": "https://github.com/rickstaa/ros-gazebo-gym-ws#ros-gazebo-gym-workspace",
  "bugs": {
    "url": "https://github.com/rickstaa/ros-gazebo-gym/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickstaa/ros-gazebo-gym-ws.git"
  },
  "license": "MIT",
  "author": "Rick Staa",
  "scripts": {
    "prepare": "husky install",
    "sort": "sort-package-json",
    "sort:py": "isort ."
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "!(CHANGELOG)*.md": "./node_modules/.bin/remark . -o --"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.6",
    "lint-staged": "15.2.0",
    "release-please": "16.8.0",
    "remark": "15.0.1",
    "remark-cli": "12.0.0",
    "remark-lint": "9.1.2",
    "remark-preset-lint-recommended": "6.1.3",
    "sort-package-json": "2.6.0"
  }
}
